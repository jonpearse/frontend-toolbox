{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "array-bracket-spacing": [ 2, "always" ],
    "arrow-parens": 2,
    "arrow-spacing": 2,
    "block-scoped-var": 1,
    "brace-style": [ 2, "allman", {
        "allowSingleLine": false
    }],
    "camelcase": [ 2, {
      "properties": "never" }
    ],
    "comma-dangle": [1, "always"],
    "comma-style": [2, "last"],
    "complexity": [2, 5],
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "indent": 2,
    "max-len": [2, 100, {
        "ignoreComments": true,
        "ignoreUrls": true
    }],
    "new-cap": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-console": 0,
    "no-debugger": 0,
    "no-self-compare": 1,
    "no-use-before-define": 1,
    "no-var": 2,
    "quotes": [2, "single"],
    "space-before-blocks": [2, "always"],
    "space-infix-ops": 2,
    "valid-jsdoc": [1, {
        "requireReturn": false
    }],
    "wrap-iife": 2,
  }
}
